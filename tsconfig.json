{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "baseUrl": "./",
    "paths": {
      "*": [
        "./src/*"
      ]
    },
    "outDir": "./dist",
    "forceConsistentCasingInFileNames": true,
    "strict": true
  },
  "include": [
    ".eslintrc.cjs",
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
